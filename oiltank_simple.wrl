DEF hachimaki Transform {
	translation 0 1.4 0
	children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor 1 1 1
				}
			}
			geometry Cylinder {radius 2.65 height 1}
		}
	]
}

DEF tank Transform {
	translation 0 0 0
	children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor 0.4 0.4 0.4
				}
				#Textureを張る画像を切り替えることでモニタ内の画像を切り替えられる。
#				texture ImageTexture{
#					url "kuchi.png"
#				}
			}
			geometry Cylinder {radius 2.5 height 5}
		}
	]
}

