#VRML V2.0 utf8


Background {
	skyColor 1 1 1
}



#目
DEF Eye1 Transform {
	translation -0.8 0 2.5
	children [
		Shape {
		  geometry Sphere {
		    radius 0.6
		  }
		  appearance Appearance {
			material Material {
#				diffuseColor 1 1 1
#				diffuseColor 1 0.2 0.2
#				emissiveColor 0.5 0 0
				diffuseColor 1 0 0
				emissiveColor 0.5 0 0
			}
		  }
		}
	]
}

DEF Eye2 Transform {
	translation 0.8 0 2.5
