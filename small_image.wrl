#VRML V2.0 utf8

Background {
	skyColor 0.8 0.8 1
}

Viewpoint {
	position 0 4 20
}


DEF Engineer Transform {
	translation 1 0 -1
	children [

		#頭
		Transform {
			translation 0 6.3 0
			children [
				Shape {
					geometry Sphere {
						radius 2.5
					}
					appearance Appearance {
						material Material {
							diffuseColor 0.2 0.2 0.2
						}
					}
				}
			]
		}

		#肩
		Transform {
			translation 0 1.8 0
			children [
				Shape {
					geometry Sphere {
						radius 2.5
					}
					appearance Appearance {
						material Material {
							diffuseColor 0.2 0.2 0.2
						}
					}
				}
			]
		}

		#胴体
		Transform {
			translation 0 0 0
			children [
				Shape {
					appearance Appearance {
						material Material {
							diffuseColor 0.2 0.2 0.2
						}
					}
					geometry Cylinder {radius 2.5 height 3.6}
				}
			]
		}

		#ハチマキ
		Transform {
			translation 0 6.8 0
			children [
				Shape {
					appearance Appearance {
						material Material {
							diffuseColor 1 1 1
						}
					}
					geometry Cylinder {radius 2.5 height 0.5}
				}
			]
		}
	]
}




