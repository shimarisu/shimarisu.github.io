#VRML V2.0 utf8

Background {
	skyColor 0.8 0.8 1
}

DEF tank Transform {
	translation 0 0 0
	rotation 1 0 0 0.3
	children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor 0.6 0.6 0.6
				}
			}
			geometry Cylinder {radius 2.5 height 5}
		}
	]
}



#ロゴ
DEF Secure Transform {
	translation -1.3 -0.5 5
	children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor 1 1 1
				}
			}
			geometry Text {
				string "INFR"
				fontStyle FontStyle {
					family "Helvetica"
					size 1.3
				}
			}
		}
	]
}
